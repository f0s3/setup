{
  "files.autoSave": "afterDelay",
  "terminal.integrated.fontFamily": "DejaVu Sans Mono, monospace",
  "projectManager.git.baseFolders": ["$home/Projects"],
  "projectManager.sortList": "Recent",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "editor.fontFamily": "DVCode",
  "editor.fontLigatures": true,
  "editor.tabSize": 2,
  "editor.unicodeHighlight.allowedLocales": {
    "ru": true
  },
  "editor.stickyScroll.enabled": true,
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.organizeImports": true
  },
  "terminal.external.osxExec": "warp.app",
  "workbench.colorTheme": "Andromeda Italic Bordered",
  "workbench.iconTheme": "material-icon-theme",
  "editor.fontVariations": false,
  "editor.fontSize": 16,
  "editor.minimap.enabled": false,
  "zenMode.hideLineNumbers": false,
  "turboConsoleLog.logMessagePrefix": "",
  "turboConsoleLog.logMessageSuffix": ": ",
  "turboConsoleLog.delimiterInsideMessage": "->",
  "turboConsoleLog.addSemicolonInTheEnd": true,
  "editor.suggest.insertMode": "replace",
  "extensions.autoUpdate": "onlyEnabledExtensions",
}

